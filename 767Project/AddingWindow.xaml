<Window x:Name="wndowAdding" x:Class="_767Project.AddingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_767Project"
        mc:Ignorable="d"
        Title="Add A Program To Block" Height="178.377" Width="328.989">
    <Grid Margin="0,0,2,-5">
        <TextBox x:Name="txtAdd" HorizontalAlignment="Left" Height="23" Margin="10,53,0,0" TextWrapping="Wrap" Text="Program to Block" VerticalAlignment="Top" Width="200" MaxLines="2" MinLines="1" GotFocus="txtAdd_GotFocus"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="224,53,0,0" VerticalAlignment="Top" Width="76" Click="btnAdd_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="225,97,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
